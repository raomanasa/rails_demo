<h1> Here are our articles <h1>
<%= link_to 'New Article' ,  new_article_path %>
<%=form_with model: @article %>

<% @articles.each do |article|%> 
<%= article.title %>
<% form.lable :title%>
      <%form.text_field :title%>
      <%form.lable :content%>
      <%form.text_area :content%>
      <%form.submit "create" %>

<% end %>